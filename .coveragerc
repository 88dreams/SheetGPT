[run]
source = src
omit =
    src/scripts/*
    */python?.?/*
    */__pycache__/*
    */site-packages/*
    */virtual/*
    */migrations/*
    */alembic/*
    */test*.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise NotImplementedError
    if __name__ == .__main__.:
    pass
    raise ImportError
    except ImportError:

[html]
directory = coverage_html_report