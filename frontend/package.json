{
  "name": "sheetgpt-frontend",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "jest",
    "test:fixed": "jest -t 'FieldItem|FieldHelpTooltip|useRecordNavigation|useUiState|useFieldMapping|useDataManagement'",
    "test:coverage": "jest --coverage -t 'FieldItem|FieldHelpTooltip|useRecordNavigation|useUiState|useFieldMapping|useDataManagement'",
    "test:no-types": "jest --no-typecheck",
    "test:entitylist": "jest --config=jest.entitylist.config.js --testPathPattern=/tests"
  },
  "dependencies": {
    "@ant-design/icons": "^5.0.0",
    "@headlessui/react": "^1.7.17",
