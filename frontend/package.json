    "@types/react-router-dom": "^5.3.3",
    "antd": "^5.0.0",
    "axios": "^1.6.2",
    "date-fns": "^4.1.0",
    "papaparse": "^5.4.1",
    "react": "^18.3.1",
    "react-beautiful-dnd": "^13.1.1",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.3.1",
    "react-icons": "^4.12.0",
    "react-markdown": "^10.0.0",
    "react-router-dom": "^6.29.0",
    "uuid": "^11.1.0",
    "zustand": "^4.4.6"
